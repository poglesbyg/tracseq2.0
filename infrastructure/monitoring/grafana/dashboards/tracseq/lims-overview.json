      "title": "Service Status Details",
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Time 1": true,
              "Time 2": true,
              "job": true,
              "job 1": true,
              "job 2": true,
              "__name__": true,
              "__name__ 1": true,
              "__name__ 2": true
            },
            "indexByName": {},
            "renameByName": {
              "instance": "Endpoint",
              "instance 1": "Endpoint",
              "instance 2": "Endpoint",
              "Value #A": "Status",
              "Value #B": "Response Time"
            }
          }
        },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "Endpoint": {
                "aggregations": [],
                "operation": "groupby"
              },
              "Status": {
                "aggregations": ["lastNotNull"],
                "operation": "aggregate"
              },
              "Response Time": {
                "aggregations": ["lastNotNull"],
                "operation": "aggregate"
              }
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "Service",
            "mode": "string",
            "reduce": {
              "reducer": "sum"
            },
            "replaceFields": false,
            "sourceField": "Endpoint",
            "binary": {
              "left": "Endpoint",
              "operator": "+",
              "right": ""
            }
          }
        },
        {
          "id": "filterFieldsByName",
          "options": {
            "include": {
              "pattern": "Service|Status|Response Time"
            }
          }
        },
        {
          "id": "convertFieldType",
          "options": {
            "conversions": [
              {
                "targetField": "Service",
                "destinationType": "string",
                "options": {
                  "trimMode": "both"
                }
              }
            ],
            "fields": {}
          }
        }
      ],
      "type": "table"
