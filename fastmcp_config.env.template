# TracSeq 2.0 FastMCP Environment Configuration Template
# Copy this file to .env and configure your API keys

# AI Model API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# FastMCP Configuration  
FASTMCP_LOG_LEVEL=INFO
FASTMCP_DEFAULT_MODEL=claude-3-sonnet-20240229

# Laboratory Service Ports
LAB_SERVICE_PORT=8000
RAG_SERVICE_PORT=8001
AGENT_SERVICE_PORT=8003
GATEWAY_SERVICE_PORT=8005

# Laboratory Settings
LAB_CONFIDENCE_THRESHOLD=0.85
LAB_PROCESSING_TIMEOUT=300

# Database URLs (optional)
DATABASE_URL=postgresql://user:password@localhost:5432/tracseq
REDIS_URL=redis://localhost:6379 